    "profile_execution": false,
    "cache_enabled": true,
    "parallel_processing": true,
    "max_worker_threads": 4,
    "response_timeout": 30.0
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file_enabled": true,
    "file_path": "fsot_system.log",
    "max_file_size_mb": 100,
    "backup_count": 5,
    "console_enabled": true
